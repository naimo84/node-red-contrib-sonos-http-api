{
  "name": "node-red-contrib-sonos-http-api",
  "version": "0.2.9",
  "description": "Node RED nodes for Sonos devices, using sonos-http-api",
  "main": "index.js",
  "author": "naimo84",
  "license": "MIT",
  "keywords": [
    "node-red",
    "sonos",
    "sonos-http-api",
    "music"
  ],
  "node-red": {
    "nodes": {
      "sonos-http-api-config": "dist/sonos-config.js",
      "sonos-http-api-control": "dist/sonos-control.js",
      "sonos-http-api-status": "dist/sonos-status.js",
      "sonos-http-api-queue": "dist/sonos-queue.js",
      "sonos-http-api-notify": "dist/sonos-notify.js",
      "sonos-http-api-get-queue": "dist/sonos-get-queue.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naimo84/node-red-contrib-sonos-http-api"
  },
  "bugs": {
    "url": "https://github.com/naimo84/node-red-contrib-sonos-http-api/issues"
  },
  "scripts": {
    "start": "gulp && node-red",
    "test": "mocha \"test/**/*_spec.js\"",
    "release": "gulp && standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit"
    }
  },
  "dependencies": {
    "jsbn": "^1.1.0",
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^12.7.9",
    "@types/node-red": "^0.20.0",
    "commitlint": "^8.2.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^5.0.1",
    "husky": "^3.0.8",
    "mocha": "6.2.0",
    "node-red": "3.0.2",
    "node-red-node-test-helper": "^0.3.0",
    "should": "13.2.3",
    "standard-version": "^7.0.0",
    "typescript": "^3.6.3"
  },
  "commitlint": {
    "format": {
      "helpUrl": "test"
    },
    "rules": {
      "body-leading-blank": [
        1,
        "always"
      ],
      "footer-leading-blank": [
        1,
        "always"
      ],
      "header-max-length": [
        2,
        "always",
        72
      ],
      "scope-case": [
        2,
        "always",
        "lower-case"
      ],
      "subject-case": [
        2,
        "never",
        [
          "sentence-case",
          "start-case",
          "pascal-case",
          "upper-case"
        ]
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-full-stop": [
        2,
        "never",
        "."
      ],
      "type-case": [
        2,
        "always",
        "lower-case"
      ],
      "type-empty": [
        2,
        "never"
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  }
}
